);\r\n  const [openCalendar,setOpenCalendar] = useState(false)\r\n  const [opensdCalendar,setOpensdCalendar] = useState(false)\r\n  const [loading,setLoading] = useState(true)\r\n  if (openCalendar == true && opensdCalendar == true){\r\n    setOpenCalendar(false)\r\n    setOpensdCalendar(false)\r\n  }\r\n  const getDefaultHotels = async () => {\r\n      const check = localStorage.getItem('hotels')\r\n      if (check){\r\n        setHotels(JSON.parse(check))\r\n        setLoading(false)\r\n      }\r\n      else{\r\n        const gethotels =  await fetch('/api/hotels');\r\n      const data = await gethotels.json()\r\n      setHotels(data)\r\n      setLoading(false)\r\n      localStorage.setItem('hotels',JSON.stringify(data))\r\n      }\r\n\r\n  }\r\n  filterNumofPeople = hotels.filter(each => each.numofpeople >= parseInt(numofpeople))\r\n \r\n  useEffect(() => {\r\n    getDefaultHotels()\r\n    \r\n  },[])\r\n\r\n  \r\n  const {t,i18n} = props\r\n  return (\r\n    <>\r\n      <div className='container'>\r\n        <div className=\"hotels\">\r\n          <div className=\"hotelsearch\">\r\n            <h2 className='smalltitle'>{t('hotelsearch.1')}</h2>\r\n          </div>\r\n          <div className=\"hotelnav\">\r\n            <Link to='/' className='tohome'>\r\n            <h2>{t('home.1')}</h2>\r\n            </Link>\r\n            <h2>/</h2>\r\n            <h2>{t('hotelsearch.1')}</h2>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    <div className=\"filters\">\r\n      <div className=\"container\">\r\n        <div className=\"filter\">\r\n        <Link to={''}>\r\n         <p style={{color:'orange'}}>{t('default.1')}</p>\r\n        </Link>\r\n        <Link to={filterNumofPeople.length == 0 ? '' : 'low-to-high'} style={{color:'black'}}>\r\n         <p>{t('low.1')}</p>\r\n        </Link>\r\n         <Link style={{color:'black'}} to={'high-to-low'}>\r\n         <p>{t('high.1')}</p>\r\n        </Link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div className=\"allhotels\">\r\n      <div className=\"container\">\r\n        <div className=\"items\">\r\n            {loading && <PulseLoader className='spiner' />}\r\n            <div className=\"gridhotels\">\r\n            {filterNumofPeople.map(hotel => {\r\n              return(\r\n                <div className='cards' key={hotel.id}>\r\n                    <img className='pictures' src={hotel.frimg}  alt=\"\" />\r\n                    <h3 className='text'>{hotel.title}</h3>\r\n                    <p className='shortdeschotel'>{t('shortdesc.1')}</p>\r\n                    <h3 className='price'>{t('starting.1')} {hotel.price}$</h3>\r\n                     <Link to={'/hotels/'+ hotel.id} className='links'>\r\n                     <p>{t('details.1')}</p>\r\n                     </Link> \r\n                    <hr  width='30px' color='orange' className='hrline'/>\r\n                </div>\r\n                \r\n                )\r\n              })}\r\n             \r\n              \r\n          </div>\r\n           \r\n          <div className=\"reservation\">\r\n            <h2 style={{textTransform:'uppercase'}}>{t('reservation.1')}</h2>\r\n            <p>TO: {range}$</p>\r\n            <input type=\"range\" min='0' max=\"500\" step=\"50\" value={range} onChange={(e)=> {setRange(e.target.value)}} />\r\n            <button className='submit' type='submit'>{t('filter.1')}</button>\r\n            \r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Hotelssection","C:\\Users\\lelic\\Desktop\\FiveStar-Booking\\frontend\\src\\components\\errorcomponents\\Footersection.jsx",["239","240","241"],[],"\r\nimport React from 'react'\r\nimport './footersection.css'\r\nimport paypal from '../../assets/footerassets/paypal.png'\r\nimport master from '../../assets/footerassets/master.png'\r\nimport visa from '../../assets/footerassets/visa.png'\r\nimport { useEffect,useState } from 'react'\r\nfunction Footersection(props) {\r\n  const {t,i18n} = props\r\n\r\n  const [infos,setInfos] = useState([])\r\n  useEffect( ()=> {\r\n      getCompanyInfos()\r\n     \r\n    },[])\r\n\r\n  const getCompanyInfos = async() =>{\r\n      const info = await fetch('http://127.0.0.1:8000/api/infos')\r\n      const data = await info.json()\r\n      setInfos(data)}\r\n  \r\n   let id, email, addres, phoneNum;\r\n      { infos.map(e => {\r\n        return(\r\n          {id,email,addres,phoneNum} = e\r\n        )\r\n    })}\r\n  return (\r\n    <div className=\"footer\">\r\n        <div className=\"container\">\r\n            <div className=\"columns\">\r\n                <div className=\"contactcol\">\r\n                 <h2>{t('contact.1')}</h2>\r\n                 <p>{addres}</p>\r\n                 <p>{email}</p>\r\n                 <p>{phoneNum}</p>\r\n                 <div className=\"paycards\">\r\n                    <img src={paypal} alt=\"\" />\r\n                    <img src={master} alt=\"\" />\r\n                    <img src={visa} alt=\"\" />\r\n                 </div>\r\n                </div>\r\n                <div className=\"vintage\">\r\n                    <h2 style={{textTransform:'uppercase'}}>{t('vintage.1')}</h2>\r\n                    <p>{t('shortdesc.1')}</p>\r\n            \r\n                </div>\r\n                <div className=\"newletter\">\r\n                  <h2 style={{textTransform:'uppercase'}}>{t('newsletter.1')}</h2>\r\n                  <input className='emailsend' type=\"email\" placeholder='@email' />\r\n                  <button className='gobtn'>GO</button>\r\n                </div>\r\n                \r\n            </div>\r\n        </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Footersection","C:\\Users\\lelic\\Desktop\\FiveStar-Booking\\frontend\\src\\components\\errorcomponents\\Errorsetion.jsx",["242"],[],"import React from 'react'\r\nimport Preheader from '../homecomponents/Preheader'\r\nimport Navigation from '../homecomponents/Navigat